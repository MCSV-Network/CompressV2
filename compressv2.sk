		set {_uncp.item2} to "smooth basalt" parsed as itemtype

	if {_uncp.item} is "dripstone_block":
		set {_uncp.item2} to "dripstone block" parsed as itemtype

	if {_uncp.item} is "magma_block":
		set {_uncp.item2} to "magma block" parsed as itemtype

	if {_uncp.item} is "end_stone":
		set {_uncp.item2} to "end stone block" parsed as itemtype

	if {_uncp.item} is "clay_block":
		set {_uncp.item} to "clay"
		set {_uncp.item2} to "clay block" parsed as itemtype

		
	#invlock起動
	set {cpv2.invlock.%{_p}%} to true
	#解凍開始
	set {_tmp.item} to sunflower named "&e解凍中..."
	set 1st line of lore of {_tmp.item} to "&cERROR! (Code issue)"
	set {_n} to amount of {compressed_%{_uncp.item}%} in {_p}'s inventory
	if {_uncp.item} is "bone" or "blaze_rod":
		debug({_p}, "check old compress item")
		set {_n2} to amount of {complessed_%{_uncp.item}%} in {_p}'s inventory
		debug({_p}, "amount: %{_n2}%")
		debug({_p}, "i2: %{_uncp.item2}%")
		olduncp({_p}, {_n2}, {_uncp.item}, "%{_uncp.item2}%")
	if {_n} >= 1:
		set {_tmp.amount} to {_n} * 64
		#?fix ender pearl amount
		if {_uncp.item} is "ender_pearl":
			set {_tmp.amount} to {_n} * 16
		loop {_n} times:
			remove 1 of {compressed_%{_uncp.item}%} from {_p}'s inventory
			#!空きなかったらその時点で解凍が中断されるので残りの圧縮されたアイテムはスキップされる
			#TODO: はよ直せ
			if {_p} doesn't have enough space for 64 of stone:
				send "&cError! インベントリの空きがありません!解凍が中断されました!" to {_p}
				delete {cpv2.invlock.%{_p}%}
				stop
			give {_num} of {_uncp.item2} to {_p}
			wait a tick
		send "%{cpv2.prefix}% &aSuccess! &e解凍が完了しました!" to {_p}
		if {_n2} is set:
			add {_n2} to {_n}
		send "&b解凍されたアイテム: &e%{_uncp.item}% &7(%{_n}%st)" to {_p}
		#invlock解除
		delete {cpv2.invlock.%{_p}%}
		stop
	if {_n} = 0:
		send "%{cpv2.prefix}% &cError! 対象のアイテムが見つかりませんでした!" to {_p}
		#invlock解除
		delete {cpv2.invlock.%{_p}%}
		stop

function olduncp(p: player, a: number, i: text, i2: text):
	debug({_p}, "a: %{_a}% i: %{_i}% i2: %{_i2}%")
	set {_i3} to {_i2} parsed as itemtype
	debug({_p}, "i3(parsed variable): %{_i3}%")
	loop {_a} times:
		remove 1 of {complessed_%{_i}%} from {_p}'s inventory
		#!空きなかったらその時点で解凍が中断されるので残りの圧縮されたアイテムはスキップされる
		#TODO: はよ直せ
		if {_p} doesn't have enough space for 64 of stone:
			send "&cError! インベントリの空きがありません!解凍が中断されました!" to {_p}
			delete {cpv2.invlock.%{_p}%}
			stop
		give 64 of {_i3} to {_p}
		wait a tick
#!=======[FUNCTION]=======


