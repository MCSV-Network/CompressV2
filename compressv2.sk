#定義づけ(prefix)
Variables:
	{cpv2.prefix} = "&e[&bCOMPRESS-V2&e]&r"
options:
	version: 2.60
	cpv2api: https://github.com/MCSV-Network/CompressV2/raw/main/cpv2version.txt

#*変数追加用
command /setcompresseditem [<text>]:
	permission: admin
	trigger:
		set {_name} to arg-1
		send "&7debug: %{_name}%"
		set {compressed_%{_name}%} to player's tool
		send "Success. name: {compressed_%{_name}%} - %{compressed_%{_name}%}%"
		stop
command /delcompressitem [<text>]:
	permission: admin
	trigger:
		set {_name} to arg-1
		send "&7debug: %{_name}%"
		delete {compressed_%{_name}%}
		send "Success. name: {compressed_%{_name}%} - %{compressed_%{_name}%}%"
		stop
#?debug
command /isfoundcpv2item [<text>]:
	permission: admin
	trigger:
		set {_i} to arg-1
		send "%{compressed_%{_i}%}%"
#読み込み時のメッセージ
on load:
	send "&b=================" to console
	send "&eCompress&bV2 ({@version})" to console
	send "Created by ringoXD" to console
	send "Github: &bhttps://github.com/MCSV-Network/CompressV2" to console
	send "&b=================" to console
	send "[CompressV2] Checking Updates..." to console
	set {_v} to text from url "{@cpv2api}"
	if {_v} is not set:
		send "[CompressV2] Failed to check new version! Check internet connection." to console
		stop
	if {_v} = "{@version}":
		send "[CompressV2] No updates aviliable." to console
	else:
		send "[ComprssV2] New version aviliable! {@version} -> %{_v}%" to console
		send "Download: https://github.com/MCSV-Network/CompressV2/" to console	

#?For CommandPanels plugin
command /compressv2gui:
	aliases: /cpv2gui
	trigger:
		player command "/cp cpv2gui"
command /uncompressv2gui:
	aliases: /uncpv2gui
	trigger:
		player command "/cp uncpv2gui"
