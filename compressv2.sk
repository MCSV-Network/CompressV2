#定義づけ(prefix)
Variables:
	{cpv2.prefix} = "&e[&bCOMPRESS-V2&e]&r"

#*変数追加用
command /setcompresseditem [<text>]:
	permission: admin
	trigger:
		set {_name} to arg-1
		send "&7debug: %{_name}%"
		set {compressed_%{_name}%} to player's tool
		send "Success. name: {compressed_%{_name}%} - %{compressed_%{_name}%}%"
		stop
command /delcompressitem [<text>]:
	permission: admin
	trigger:
		set {_name} to arg-1
		send "&7debug: %{_name}%"
		delete {compressed_%{_name}%}
		send "Success. name: {compressed_%{_name}%} - %{compressed_%{_name}%}%"
		stop
#読み込み時のメッセージ
on skript load:
	send "&b=================" to console
	send "&eCompress&bV2 &7- &av0.5" to console
	send "Created by ringoXD" to console
	send "Github: &bhttps://github.com/MCSV-Network/CompressV2" to console
	send "&b=================" to console
